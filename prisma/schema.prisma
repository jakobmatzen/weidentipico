generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DIRECT_URL")
}

model users {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
  username  String
  password  String
}
